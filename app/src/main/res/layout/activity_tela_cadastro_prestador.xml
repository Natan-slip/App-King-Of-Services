<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="410dp"
    android:layout_height="wrap_content"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="1dp"
    android:fillViewport="true"
    android:layout_gravity="center"
    android:layout_marginBottom="10dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context=".TelaCadastroPrestador">

        <androidx.cardview.widget.CardView
            android:id="@+id/card_view_profile"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginTop="59dp"
            android:foregroundGravity="center"
            app:cardCornerRadius="50dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.48"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/iv_profile"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:contentDescription="TODO"
                android:scaleType="centerCrop"
                android:src="@drawable/foto_perfil" />

        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/tv_trocar_foto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/roboto"
            android:text="Trocar foto"
            android:textColor="@color/colorPrimary"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.48"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/card_view_profile" />

        <EditText
            android:id="@+id/et_nomeCadastroPrestador"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_marginStart="54dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="55dp"
            android:inputType="textPersonName"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/name_24"
            android:paddingStart="10dp"
            android:paddingEnd="12dp"
            android:drawablePadding="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_nomeCadastroPrestador"
            android:maxLines="1"/>

        <EditText
            android:id="@+id/et_senhaCadastroPrestador"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_marginStart="54dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="55dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/lock_24"
            android:inputType="textPassword"
            android:paddingStart="10dp"
            android:paddingEnd="12dp"
            android:drawablePadding="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_senhaCadastroPrestador"
            android:maxLines="1"/>

        <EditText
            android:id="@+id/et_emailCadastroPrestador"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_marginStart="54dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="55dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/email_24"
            android:inputType="textEmailAddress"
            android:paddingStart="10dp"
            android:paddingEnd="12dp"
            android:drawablePadding="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_emailCadastroPrestador"
            android:maxLines="1"/>

        <TextView
            android:id="@+id/tv_emailCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="E-mail"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.14"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_dataNascimentoCadastroPrestador" />

        <TextView
            android:id="@+id/tv_nomeCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            android:fontFamily="@font/roboto"
            android:text="Nome"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.14"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_trocar_foto" />

        <RadioGroup
            android:id="@+id/radioGroup_cadastroPrestador"
            android:layout_width="333dp"
            android:layout_height="47dp"
            android:layout_marginStart="75dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="25dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_generoCadastroPrestador">

            <RadioButton
                android:id="@+id/radio_masculino"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:buttonTint="@color/colorPrimaryDark"
                android:drawablePadding="6dp"
                android:drawableTint="@color/colorPrimary"
                android:text="Masculino" />

            <RadioButton
                android:id="@+id/radio_feminino"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:buttonTint="@color/colorPrimaryDark"
                android:drawablePadding="6dp"
                android:drawableTint="@color/colorPrimary"
                android:text="Feminino" />
        </RadioGroup>

        <TextView
            android:id="@+id/tv_estadoCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="Estado"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.15"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_cepCadastroPrestador" />

        <TextView
            android:id="@+id/tv_dataNascimentoCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="Data Nascimento"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.18"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_nomeCadastroPrestador" />

        <TextView
            android:id="@+id/tv_generoCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/roboto"
            android:text="GenÃªro"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.14"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_senhaCadastroPrestador" />

        <EditText
            android:id="@+id/et_cepCadastroPrestador"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_marginStart="59dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="55dp"
            android:inputType="numberDecimal"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/localizacao_24"
            android:paddingStart="10dp"
            android:paddingEnd="12dp"
            android:drawablePadding="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_cepCadastroPrestador"
            tools:ignore="MissingConstraints"
            android:maxLines="1"/>

        <TextView
            android:id="@+id/tv_cepCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="CEP"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.16"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/radioGroup_cadastroPrestador"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/tv_senhaCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="Senha"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.14"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_emailCadastroPrestador" />

        <EditText
            android:id="@+id/et_dataNascimentoCadastroPrestador"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_marginStart="54dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="55dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/calendario_24"
            android:paddingStart="10dp"
            android:paddingEnd="12dp"
            android:drawablePadding="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_dataNascimentoCadastroPrestador"
            android:focusable="false"
            android:maxLines="1"/>

        <EditText
            android:id="@+id/et_estadoCadastroPrestador"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_marginStart="54dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="55dp"
            android:inputType="textPersonName"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/localizacao_24"
            android:paddingStart="10dp"
            android:paddingEnd="12dp"
            android:drawablePadding="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_estadoCadastroPrestador"
            android:maxLines="1"/>

        <TextView
            android:id="@+id/tv_cidadeCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="Cidade"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.15"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_estadoCadastroPrestador" />

        <TextView
            android:id="@+id/tv_bairroCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/roboto"
            android:text="Bairro"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.15"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_cidadeCadastroPrestador" />

        <TextView
            android:id="@+id/tv_ruaCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:fontFamily="@font/roboto"
            android:text="Rua"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.151"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_bairroCadastroPrestador" />

        <TextView
            android:id="@+id/tv_complementoCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:fontFamily="@font/roboto"
            android:text="Complemento"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.17"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_numeroCadastroPrestador" />

        <TextView
            android:id="@+id/tv_numeroCadastroPrestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:fontFamily="@font/roboto"
            android:text="Numero"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.154"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_ruaCadastroPrestador" />

        <EditText
            android:id="@+id/et_cidadeCadastroPrestador"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_marginStart="55dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="54dp"
            android:inputType="textPersonName"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/localizacao_24"
            android:paddingStart="10dp"
            android:paddingEnd="12dp"
            android:drawablePadding="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_cidadeCadastroPrestador"
            android:maxLines="1"/>

        <EditText
            android:id="@+id/et_bairroCadastroPrestador"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_marginStart="54dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="55dp"
            android:inputType="textPersonName"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/localizacao_24"
            android:paddingStart="10dp"
            android:paddingEnd="12dp"
            android:drawablePadding="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_bairroCadastroPrestador"
            android:maxLines="1"/>

        <EditText
            android:id="@+id/et_ruaCadastroPrestador"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_marginStart="54dp"
            android:layout_marginTop="17dp"
            android:layout_marginEnd="55dp"
            android:inputType="textPersonName"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/localizacao_24"
            android:paddingStart="10dp"
            android:paddingEnd="12dp"
            android:drawablePadding="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_ruaCadastroPrestador"
            android:maxLines="1"/>

        <EditText
            android:id="@+id/et_numeroCadastroPrestador"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_marginStart="54dp"
            android:layout_marginTop="18dp"
            android:layout_marginEnd="55dp"
            android:inputType="numberDecimal"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/localizacao_24"
            android:paddingStart="10dp"
            android:paddingEnd="12dp"
            android:drawablePadding="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_numeroCadastroPrestador"
            android:maxLines="1"/>

        <EditText
            android:id="@+id/et_complementoCadastroPrestador"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_marginStart="54dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="55dp"
            android:layout_marginBottom="65dp"
            android:background="@drawable/edit_text_login_background"
            android:drawableStart="@drawable/localizacao_24"
            android:paddingStart="10dp"
            android:paddingEnd="12dp"
            android:drawablePadding="12dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_complementoCadastroPrestador"
            android:maxLines="1"/>

        <Button
            android:id="@+id/btn_Prestador"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="Salvar"
            android:layout_marginBottom="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_complementoCadastroPrestador"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>